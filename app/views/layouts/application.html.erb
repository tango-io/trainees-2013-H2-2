
<!DOCTYPE html>
<body>
  <head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />

    <title><%= content_for?(:title) ? yield(:title) : "Kickstarter" %></title>

    <%=  stylesheet_link_tag "application"  %>
    <%=  javascript_include_tag "application" %>
    <%=  csrf_meta_tags %>

    <script>
      $(function() {
        $( "#datepicker" ).datepicker();
      });
    </script>
  </head>


  <%= render :partial => 'layouts/header', :formats=>[:html], :handlers=>[:haml] %>
  <div class = "container content content-into">
    <%= yield %>
  </div>
  <%= render :partial => 'layouts/footer', :formats=>[:html], :handlers=>[:haml] %>



</body>
</html>

